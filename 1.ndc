let p1 = "";
let p2 = "";
let p3 = "";
// input := "xBxAAABCDxCC";
let d = %{
  "A": 0,
  "B": 1,
  "C": 3,
  "D": 5,
  "x": 0,
};

fn calc(input, size) {
  let sum = 0;
  for i in 0..(input.len()), if i % size == 0 {
    let sub = 0;
    for ii in 0..size {
      sub += d[input[i + ii]];
    }
    let cc = [1 for x in input[i..i+size], if x != "x"].sum();
    let add = cc * (cc - 1);
    sum += sub + add;
  }
  return sum;
}

print(calc(p1, 1));
print(calc(p2, 2));
print(calc(p3, 3));
